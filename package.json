{
  "name": "firegit",
  "version": "1.0.0",
  "description": "Git Repositories Manage System Based on PHP.",
  "main": "index.js",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "iview": "^0.9.14",
    "jquery": "^2.2.4",
    "vue": "^1.0.28"
  },
  "devDependencies": {
    "rimraf": "^2.5.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild.js": "rm ./dist/ -rf && mkdir ./dist/",
    "build.js.jquery": "mkdir ./dist/jquery/ && cp node_modules/jquery/dist/jquery.min.js ./dist/jquery/",
    "build.js.iview": "mkdir ./dist/iview/js ./dist/iview/css/ -p && cp node_modules/iview/dist/iview.min.js ./dist/iview/js/ && cp node_modules/iview/dist/styles/* ./dist/iview/css/ -r",
    "build.js.vue": "mkdir ./dist/vue/ && cp node_modules/vue/dist/vue.js ./dist/vue/",
    "build.js": "npm run build.js.vue && npm run build.js.iview && npm run build.js.jquery",
    "build": "npm run build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/huimang/firegit.git"
  },
  "keywords": [
    "git",
    "php"
  ],
  "author": "ronnie <ronnie@huimang.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/huimang/firegit/issues"
  },
  "homepage": "https://gitlab.com/huimang/firegit#README"
}
